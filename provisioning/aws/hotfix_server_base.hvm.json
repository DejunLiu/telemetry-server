{
  "ssl_user": "ubuntu",
  "ssl_key_name": "mreid",
  "ssl_key_path": "~/.ssh/aws/mreid.pem",
  "ssl_retries": 10,
  "base_dir": "/mnt/hotfix",
  "log_dir_name": "hotfix",
  "instance_type": "m3.large",
  "image": "ami-39501209",
  "security_groups": ["telemetry"],
  "region": "us-west-2",
  "placement": "us-west-2c",
  "shutdown_behavior": "stop",
  "name": "hotfix-server",
  "default_tags": {
    "Owner": "mreid",
    "Application": "telemetry-server"
  },
  "add_aws_credentials": true,
  "skip_termination": true
}
